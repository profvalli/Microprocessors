WRITE 8086 ASSEMBLY PROGRAM TO COMPUTE    ARE SIGNED 8-BIT NUMBERS. N=100 ASSUME CS AND DS ARE ALREADY INITIALISED. ASSUME NO OVERFLOW.
		MOV CX,100
		LEA BX,ADDR1
		LEA SI,ADDR2
		MOV DX,0000H  ;SUM=0
START		MOV AL,[BX] ; LOAD DATA INTO AL
		IMUL [SI]   ; SUM XY
		ADD DX,AX
		INC BX
		INC SI
		LOOP START
		HLT
